{
    "foo": {
        "prefix": "foo",
        "body": ["local ${1:foo} = ${2:foo}}", "return ${3:baz}}"]
    },
    "if_else": {
        "prefix": "ifelse",
        "body": [
            "if ${1:condition} then",
            "    ${2:-- body if true}",
            "else",
            "    ${3:-- body if false}",
            "end"
        ]
    }
}
